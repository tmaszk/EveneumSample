{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [{
    "name": "Launch EveneumSample",
    "type": "coreclr",
    "request": "launch",
    "preLaunchTask": "build",
    "program": "${workspaceRoot}/EveneumSample/bin/Debug/netcoreapp3.0/EveneumSample.dll",
    "args": [],
    "cwd": "${workspaceRoot}/EveneumSample",
    "stopAtEntry": false,
    "internalConsoleOptions": "openOnSessionStart",
    "serverReadyAction": {
      "pattern": "listening on: https?://localhost(\\S+|[0-9]+)",
      "uriFormat": "https://localhost%s",
      "action": "openExternally"
    },
    "env": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    },
    "sourceFileMap": {
      "/Views": "${workspaceRoot}/EveneumSample/Views"
    }
  }]
}